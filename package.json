{
  "name": "project",
  "version": "0.0.0",
  "description": "test app for pdp",
  "author": "Sergey Gulidov",
  "license": "MIT",
  "scripts": {
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migrate:generate": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migration:generate -n ",
    "migrate:create": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migration:create -n ",
    "migrate:run": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migration:run ",
    "migrate:revert": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migration:revert ",
    "seed:generate": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migration:generate -c seed -n ",
    "seed:create": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migration:create -c seed -n ",
    "seed:run": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migration:run -c seed",
    "seed:revert": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migration:revert -c seed",
    "build": "tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "pm2-start": "pm2 start src/main.ts",
    "start:dev": "nodemon",
    "start:debug": "nodemon --config nodemon-debug.json",
    "prestart:prod": "rimraf dist && npm run build",
    "start:prod": "node dist/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^5.4.0",
    "@nestjs/core": "^5.4.0",
    "@nestjs/passport": "^5.1.0",
    "@nestjs/platform-socket.io": "^6.0.3",
    "@nestjs/typeorm": "^5.2.2",
    "@nestjs/websockets": "^6.0.3",
    "aws-sdk": "^2.422.0",
    "bcrypt": "^3.0.3",
    "class-transformer": "^0.2.0",
    "class-validator": "^0.9.1",
    "jsonwebtoken": "^8.4.0",
    "multer": "^1.4.1",
    "multer-s3": "^2.9.0",
    "nest-router": "^1.0.9",
    "passport": "^0.4.0",
    "passport-facebook-token": "^3.3.0",
    "passport-jwt": "^4.0.0",
    "pg": "^7.8.0",
    "pm2": "^3.2.9",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^2.6.2",
    "rxjs": "^6.2.2",
    "socket.io-redis": "^5.2.0",
    "typeorm": "^0.2.12",
    "typescript": "^3.0.1"
  },
  "devDependencies": {
    "@nestjs/testing": "^5.1.0",
    "@types/express": "^4.16.0",
    "@types/jest": "^23.3.1",
    "@types/node": "^10.7.1",
    "@types/supertest": "^2.0.5",
    "jest": "^23.5.0",
    "nodemon": "^1.18.3",
    "prettier": "^1.14.2",
    "supertest": "^3.1.0",
    "ts-jest": "^23.1.3",
    "ts-loader": "^4.4.2",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.5.0",
    "tslint": "5.11.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
